{
	"$schema": "https://docs.renovatebot.com/renovate-schema.json",
	"extends": ["github>Jolt-Design/Renovate-Config"],
	"ignorePaths": ["**/node_modules/**"],
	"constraints": {
		"node": "^16"
	},
	"packageRules": [
		{
			"matchPackageNames": ["node", "@types/node"],
			"matchUpdateTypes": "major",
			"enabled": false
		}
	]
}
